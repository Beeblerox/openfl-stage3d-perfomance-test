<?xml version="1.0" encoding="utf-8"?>
<project>
	
	<meta title="OpenFLStage3DTest" package="wighawag.test.stage3d" version="1.0.0" company="Wighawag" />
    <app main="Main" path="target/openfl" file="OpenFLStage3DTest" />

    <window width="480" height="640" />
    <window orientation="portrait" fps="60" />

	<set name="SWF_VERSION" value="11.2" />
	
	<source path="source" />
	
	<haxelib name="openfl"/>
	
    <assets path="assets" exclude="nme.svg"/>

	<icon path="assets/nme.svg" />
    
    <haxelib name="openfl-stage3d" />
	 
	<!-- HXCPP DEBUGGING -->
    <haxedef name="HXCPP_DEBUG_LINK" />
    <haxedef name="DHXCPP_STACK_TRACE" />
    <haxedef name="HXCPP_STACK_LINE" />
    <haxedef name="HXCPP_CHECK_POINTER" />
    <haxedef name="HXCPP_DEBUGGER " />

</project>
